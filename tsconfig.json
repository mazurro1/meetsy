{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["dom", "dom.iterable", "esnext", "es6", "webworker"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/pageApiActions/*": ["pageApiActions/*"],
      "@/utils/*": ["utils/*"],
      "@/models/*": ["models/*"],
      "@lib": ["lib"],
      "@/redux/*": ["components/redux/*"],
      "@/lib/*": ["lib/*"],
      "@/pages/*": ["pages/*"],
      "@icons": ["components/icons"],
      "@ui": ["components/ui"],
      "@hooks": ["components/hooks"],
      "@constants": ["components/constants"],
      "@functions": ["components/functions"],
      "@Texts": ["Texts"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
